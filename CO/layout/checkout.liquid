        campoCompany.append(inputNumber);
      
      function formatNumberDoc(documento,valor) {        
        documento = documento.length+1;        
		console.log(valor);
      	numero = valor.substring(documento, valor.length);
        console.log(numero)
        return numero;       
      }
      
      // funcion para cuando tenemos los datos guardados en el checkuout
      setTimeout(function(){
      	if($(".logged-in-customer-information__avatar-wrapper")) {        
          var cedulaValor = $("#checkout_shipping_address_company").val();          
          if(cedulaValor.indexOf("C.C") >= 0){
            $("#selectCedula").val("C.C");                        
            $("#inputNumber").val(formatNumberDoc("C.C",cedulaValor));                      
          }else if(cedulaValor.indexOf("C.E") >= 0){
            $("#selectCedula").val("C.E");
            $("#inputNumber").val(formatNumberDoc("C.E",cedulaValor));          
