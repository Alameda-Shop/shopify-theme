    <style>
      .shown-if-js{
        
      }
      .shown-if-js:after{        
      }
      [data-address-field="country"] {
        /* Attribute value contains this value somewhere in it */
        display:none;
      }
      .selectCedula {
        appearance: auto !important;
        padding: 0.8em;
        border-radius: 5px;
        border-color: #d9d9d9;
        margin: 6px 0;        
    	border: 1px solid #d9d9d9 !important;
        width:50%;
      }
      .selectCity {
        appearance: auto !important;
        padding:0px;
        border-radius: 5px;
        border-color: #d9d9d9;
        margin: 6px 0;        
    	border: 0px solid #d9d9d9 !important;
        width:49%;
        float:left;
      }
      .inputNumber {
        width: 49%;
    	float: right;
	    margin: 6px 0px;        
      }
      .selectize-input {        
        height: 46px;
      }            
      [data-address-field="company"] div.field__input-wrapper label,
      [data-address-field="company"] div.field__input-wrapper #checkout_shipping_address_company,
      [data-address-field="province"],
      #checkout_shipping_address_city{
        display:none;
      }      
      [data-address-field="zip"]{
      	float:left;
      }
      [data-address-field="phone"]{
      	width:66%;
      }
      [data-address-field="city"] .selectize-control.selectCity.single:nth-child(3) {
      	padding-right:11px;
      }
